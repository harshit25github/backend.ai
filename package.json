{
  "name": "backend-ai-chat",
  "version": "1.0.0",
  "description": "REST-based backend for AI chat with OpenAI Agents SDK",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node tests/test-extractor-agent.js",
    "test:extractor": "node tests/test-extractor-agent.js",
    "test:e2e": "node tests/e2e-trip-planner.js",
    "flight:playground": "node flight-playground.js",
