  "dependencies": {
    "@openai/agents": "^0.3.4",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "zod": "^3.0.0"
